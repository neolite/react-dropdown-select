{
  "plugins": [
    "emotion",
    "@babel/plugin-proposal-class-properties",
    [
      "transform-react-remove-prop-types",
      {
        "mode": "remove",
        "ignoreFilenames": ["node_modules"]
      }
    ]
  ],
  "presets": [
    "minify",
    [
      "@babel/preset-env",
      {
        "loose": true,
        "modules": false,
        "debug": true
      }
    ],
    "@babel/preset-react"
  ]
}
